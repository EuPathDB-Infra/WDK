{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "jsx": "react",
    "strictNullChecks": true,
    "lib": [ "dom", "es2015", "es2016" ],
    "moduleResolution": "node"
  }
}
