{
  "name": "wdk",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "build-js": "webpack",
    "build-css": "mkdir -p dist/wdk/css && cd dist/wdk/css && cleancss --skip-rebase --source-map -o wdk.min.css ../../../webapp/wdk/css/wdk.css",
    "install-build-packages": "npm install --production",
    "install-test-packages": "npm install",
    "client-tests": "tape -r ts-node/register -r babel-core/register -r babel-polyfill tests/client/**/*.js",
    "test-server": "webpack-dev-server --config webpack-test.config.js test/tests.js",
    "examples-server": "cd client-examples && webpack-dev-server --content-base public/"
  },
  "private": true,
  "dependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-plugin-transform-es2015-classes": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "backbone": "^1.2.1",
    "classnames": "^2.2.0",
    "clean-css": "^3.3.3",
    "css-loader": "^0.17",
    "fixed-data-table": "^0.6",
    "flux": "^2.0.3",
    "json-stable-stringify": "^1.0.0",
    "lodash": "^3.9.3",
    "react": "^15",
    "react-dom": "^15",
    "react-addons-pure-render-mixin": "^15",
    "react-addons-shallow-compare": "^15",
    "react-addons-update": "^15",
    "react-addons-perf": "^15",
    "react-router": "^2.0.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "history": "2.0.0",
    "ts-loader": "~0.8.1",
    "typescript": "~1.8.9",
    "typings": "~1.3.1",
    "script-loader": "~0.7.0",
    "file-loader": "~0.8.5",
    "url-loader": "~0.5.7",
    "localforage": "~1.4.2"
  },
  "devDependencies": {
    "eslint": "^2.0.0",
    "eslint-plugin-react": "^5.0.0",
    "tape": "^4.0.0",
    "ts-node": "^0.9.1"
  }
}
