{
  "name": "wdk",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "build-js": "webpack",
    "build-css": "mkdir -p dist/wdk/css && cd dist/wdk/css && cleancss --skip-rebase --source-map -o wdk.min.css ../../../webapp/wdk/css/wdk.css",
    "install-build-packages": "npm install --production",
    "install-test-packages": "npm install",
    "client-tests": "tape -r ts-node/register -r babel-core/register -r babel-polyfill tests/client/**/*.js",
    "test-server": "webpack-dev-server --config webpack-test.config.js test/tests.js",
    "examples-server": "cd client-examples && webpack-dev-server --content-base public/"
  },
  "private": true,
  "dependencies": {
    "@types/flux": "^0.0.33",
    "@types/jquery.datatables": "^1.10.33",
    "@types/jqueryui": "^1.11.32",
    "@types/json-stable-stringify": "^1.0.29",
    "@types/localforage": "^0.0.33",
    "@types/lodash": "^4.14.44",
    "@types/node": "^6.0.52",
    "@types/react-addons-shallow-compare": "^0.14.17",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^2.0.41",
    "@types/spin": "^2.3.30",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.0.0",
    "backbone": "^1.2.1",
    "classnames": "^2.2.0",
    "clean-css": "^3.4.23",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "fixed-data-table": "^0.6",
    "flux": "^2.0.3",
    "history": "2.0.0",
    "json-stable-stringify": "^1.0.0",
    "localforage": "~1.4.2",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-addons-perf": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-addons-update": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "script-loader": "~0.7.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "url-loader": "~0.5.7",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "tape": "^4.6.3",
    "ts-node": "^1.7.2"
  }
}
