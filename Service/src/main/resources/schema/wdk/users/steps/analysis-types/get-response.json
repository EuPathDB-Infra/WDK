{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "hasParameters": {
        "type": "boolean"
      },
      "displayName": {
        "type": "string",
        "examples": [
          "Gene Ontology Enrichment"
        ]
      },
      "releaseVersion": {
        "type": "string",
        "examples": [
          "22"
        ]
      },
      "name": {
        "type": "string",
        "examples": [
          "go-enrichment"
        ]
      },
      "description": {
        "type": "string",
        "examples": [
          "<p>Some HTML String</p>"
        ]
      },
      "customThumbnail": {
        "type": "string",
        "examples": [
          "wdkCustomization/images/go-analysis-logo.png"
        ]
      },
      "shortDescription": {
        "type": "string",
        "examples": [
          "Find Gene Ontology terms that are enriched in your gene result."
        ]
      }
    }
  }
}
