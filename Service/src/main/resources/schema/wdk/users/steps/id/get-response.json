{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "displayName": {
      "description": "Name populated with default name if not sent in",
      "type": "string"
    },
    "shortDisplayName": {
      "type": "string"
    },
    "customName": {
      "type": "string"
    },
    "baseCustomName": {
      "type": "string"
    },
    "isCollapsible": {
      "type": "boolean"
    },
    "collapsedName": {
      "type": "string"
    },
    "hasCompleteStepAnalysis": {
      "type": "boolean"
    },
    "recordClassName": {
      "type": "string"
    },
    "isAnswerSpecComplete": {
      "type": "boolean"
    },
    "strategyId": {
      "description": "Strategy ID (always null)",
      "type": "null"
    },
    "estimatedSize": {
      "description": "unpopulated since step has not yet run",
      "type": "null"
    },
    "isValid": {
      "description": "always true here since params had to have validated",
      "type": "boolean",
      "enum": ["true"]
    },
    "createdDate": {
      "type": "string"
    },
    "lastRunTime": {
      "type": "string"
    },
    "displayPrefs": {
      "$ref": "../includes/display-prefs.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "isCollapsible",
    "hasCompleteStepAnalysis",
    "isAnswerSpecComplete",
    "isValid"
  ]
}
