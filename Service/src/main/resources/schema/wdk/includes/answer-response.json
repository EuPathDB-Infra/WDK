  "type": "object",
  "additionalProperties": false,
  "properties": {
    "records": {
      "type": "array",
      "items": {
        "$ref": "/schema/wdk/includes/record_instance-response.json"
      }
    },
    "meta": {
      "type": "object",
      "properties": {
        "recordClassName": {
          "type": "string"
        },
        "totalCount": {
          "type": "integer"
        },
        "responseCount": {
          "type": "integer"
        },
        "tables": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "attributes": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "tables",
        "meta"
      ]
    }
  },
  "required": [
    "records",
    "meta"
  ]
}
